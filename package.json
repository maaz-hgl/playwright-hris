{
  "name": "playwrighthris",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test:allure": "rm -rf allure-results && npx playwright test --reporter=allure-playwright",
    "allure:generate": "allure generate allure-results -o allure-report --clean",
    "allure:open": "allure open allure-report",
    "allure:run": "rm -rf allure-results && npx playwright test --reporter=allure-playwright && allure generate allure-results -o allure-report --clean && allure open allure-report",
    "allure:run-file": "rm -rf allure-results && npx playwright test $1 --reporter=allure-playwright && allure generate allure-results -o allure-report --clean && allure open allure-report",
    "allure:clean": "rimraf allure-results allure-report"
  },
  
